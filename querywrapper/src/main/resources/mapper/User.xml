<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.querywrapperdemo.dao.UserMapper">

    <!--根据条件获取用户列表信息-->
    <select id="getUserList" resultType="com.example.querywrapperdemo.entity.User">
        select * from t_user
        <if test="ew!=null and ''!=ew">
            ${ew.customSqlSegment}
        </if>

    </select>
</mapper>